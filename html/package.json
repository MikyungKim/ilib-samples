{
  "name": "ilib-on-html",
  "version": "1.0.0",
  "description": "",
  "main": "ilib-include.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "scan": "ilib-scanner --assembly=assembled --locales=ko-KR,es-ES ilib-include.js",
    "build": "webpack",
    "gen-res-assemble": "ilib-resbundler --assembly assembled --compiled compiled --resDir resources --outDir outResDir",
    "gen-res-dynamic": "ilib-resbundler --assembly dynamic --compiled compiled --resDir resources --outDir outResDir --locales 'ko-KR,es-ES'",
    "clean": "rm -rf assets output ilib.js ilib-include.js webpack.config.js outResDir"
  },
  "author": {
    "name": "Goun Lee",
    "email": "goun.lee@lge.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "ilib-resbundler": "^1.0.1",
    "ilib-scanner": "^1.3.1",
    "ilib-webpack-loader": "^1.3.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "webpack-cli": "^4.9.2"
  }
}